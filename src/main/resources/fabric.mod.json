{
  "schemaVersion": 1,
  "id": "zmbutilities",
  "version": "${version}",

  "name": "Zmb Utilities",
  "description": "This Mod adds various Utilities and Blocks to Minecraft!",
  "authors": [
    "Zumbsy"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT License",
  "icon": "assets/zmbutilities/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.zumbsy.zmbutilities.ZmbUtilities"
    ],
    "client": [
      "net.zumbsy.zmbutilities.ZmbUtilitiesClient"
    ]
  },
  "mixins": [
    "zmbutilities.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
